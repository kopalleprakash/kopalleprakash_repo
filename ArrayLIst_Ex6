import java.util.ArrayList;
import java.util.List;

public class ArrayLIst_Ex6 {
	public static void main(String[] args) 
	{
		List<String> colors = new ArrayList<String>();
		
		colors.add("Green");
		colors.add("Red");
		colors.add("Voilet");
		colors.add("White");
		colors.add("Purple");
		colors.add("Yellow");
		
		System.out.println(colors);
		
		colors.remove(2);
		
		System.out.println(colors);
	}	
}
