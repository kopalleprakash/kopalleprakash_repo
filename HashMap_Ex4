import java.util.*;
public class HashMap_Ex4 {
public static void main(String[] args) 
{
	
	HashMap<Integer,String> names = new HashMap<Integer,String>();
	names.put(1, "Prakash");
	names.put(2, "Abhishek");
	names.put(3, "Siva");
	names.put(4, "Mounika");
	System.out.println("This is the first Hash Map :"+names);
	names.clear();
	System.out.println("This is the first Hash Map :"+names);
}
}
