import java.util.ArrayList;
import java.util.List;
public class ArrayList_Iterate {
	

	public static void main(String[] args) 
	{
		List<String> colors = new ArrayList<String>();
		colors.add("Green");
		colors.add("Red");
		colors.add("Voilet");
		for(String color : colors )
		{
			System.out.println(color);
		}
		
	}	
	}


