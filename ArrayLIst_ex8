import java.util.*;

public class ArrayLIst_ex8 {
	
	public static void main(String[] args) {
	List<String> colors = new ArrayList<String>();
	
	colors.add("Green");
	colors.add("Red");
	colors.add("Voilet");
	colors.add("White");
	colors.add("Purple");
	colors.add("Yellow");
	
	System.out.println(colors);
	
	Collections.sort(colors);
	
	System.out.println(colors);
}
}
